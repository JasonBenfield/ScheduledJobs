<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Description>Abstractions for Scheduled Jobs</Description>
		<PackageTags>$(XtiVersion);$(XtiAppName);XTI Job Abstractions</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="XTI_Core" Version="1.10.18" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="JobErrors.Designer.cs">
			<DependentUpon>JobErrors.resx</DependentUpon>
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="JobErrors.resx">
			<LastGenOutput>JobErrors.Designer.cs</LastGenOutput>
			<Generator>PublicResXFileCodeGenerator</Generator>
		</EmbeddedResource>
	</ItemGroup>

</Project>
